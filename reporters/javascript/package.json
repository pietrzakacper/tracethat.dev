{
  "name": "tracethat.dev",
  "version": "1.1.1",
  "description": "JS/TS reporter for tracethat.dev",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "npm run build && npm run test && npm run bundle-web",
    "test": "tape ./dist/**/*.spec.js",
    "bundle-web": "rm -rf dist-web && esbuild ./dist/browser.index.js --bundle --minify --outfile=./dist-web/tracethat.min.js"
  },
  "homepage": "https://github.com/pietrzakacper/tracethat.dev",
  "keywords": [
    "devtools",
    "reporter",
    "debugging"
  ],
  "author": "Kacper Pietrzak <kacpietrzak@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "esbuild": "^0.20.1",
    "@flydotio/dockerfile": "^0.5.0",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.11.15",
    "@types/tape": "^4.13.2",
    "@types/ws": "^8.5.3",
    "tape": "^5.6.1",
    "typescript": "^4.9.3"
  }
}