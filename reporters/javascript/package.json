{
  "name": "tracethat.dev",
  "version": "1.4.0",
  "description": "JS/TS reporter for tracethat.dev",
  "main": "./dist/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build:dep": "tsup src/index.ts --dts --treeshake --format esm,cjs --clean",
    "build:web": "esbuild src/browser.index.ts --bundle --minify --sourcemap --outfile=dist/tracethat.min.js --target=es2020",
    "build": "npm run build:dep && npm run build:web",
    "dev": "npm run build:dep -- --watch",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run type-check && npm run test && npm run build",
    "test": "bun ./src/**/*.spec.*",
    "test:integration": "bun ./integration-tests/*.spec.*"
  },
  "homepage": "https://github.com/pietrzakacper/tracethat.dev",
  "keywords": [
    "devtools",
    "reporter",
    "debugging"
  ],
  "author": {
    "name": "Kacper Pietrzak",
    "email": "kacpietrzak@gmail.com"
  },
  "maintainers": [
    {
      "name": "Bartosz Legięć",
      "email": "bartosz+github@legiec.io",
      "url": "https://legiec.io"
    }
  ],
  "license": "MIT",
  "dependencies": {
    "isomorphic-ws": "^5.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "^20.11.15",
    "@types/tape": "^4.13.2",
    "@types/ws": "^8.5.3",
    "esbuild": "^0.20.1",
    "find-free-ports": "^3.1.1",
    "tape": "^5.6.1",
    "tsup": "^8.0.2",
    "typescript": "^4.9.3"
  }
}
